{
	"release-type": "simple",
	"pull-request-title-pattern": "chore${scope}: release${component} ${version}",
	"bump-minor-pre-major": true,
	"prerelease": false,
	"changelog-types": [
		{
			"type": "feat",
			"section": "Features",
			"hidden": false
		},
		{
			"type": "chore",
			"section": "Misc",
			"hidden": false
		},
		{
			"type": "fix",
			"section": "BugFixes",
			"hidden": false
		}
	],
	"packages": {
		".": {}
	}
}